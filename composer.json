{
    "name": "anourvalar/eloquent-notification",
    "description": "Laravel Notification - model layer",
    "keywords": ["anourvalar", "laravel", "eloquent", "notification"],
    "homepage": "https://github.com/AnourValar/eloquent-notification",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "anourvalar/eloquent-validation": "^3.16",
        "anourvalar/laravel-atom": "^2.23",
        "anourvalar/http-client": "^1.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0|^10.0",
        "phpstan/phpstan": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.26",
        "squizlabs/php_codesniffer": "^3.7",
        "psalm/plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {"AnourValar\\EloquentNotification\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"AnourValar\\EloquentNotification\\Tests\\": "tests/"}
    },
    "extra": {
        "laravel": {
            "providers": [
                "AnourValar\\EloquentNotification\\Providers\\AnourValarEloquentNotificationServiceProvider"
            ]
        }
    }
}
